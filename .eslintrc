{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "consistent-return": 0,
    "indent": 0,
    "camelcase": 0,
    "quotes": ["error", "single", {
      "allowTemplateLiterals": true
    }],
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0
  }
}
